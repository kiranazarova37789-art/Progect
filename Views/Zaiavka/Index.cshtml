@{
    ViewData["Title"] = "Request";
}


<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewData["Title"]</title>
</head>
<body>
   <h3>Заявки</h3>
    <table class="table table-bordered table-striped">
        <thead>
            <tr>
                <th>ID</th>
                <th>Статус</th>
                <th>Модель</th>
                <th>Номер кабинета</th>
                <th>Корпус</th>
              
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.id_zv</td>
                    <td>@item.status_zv</td>
                    <td>@item.cartridg_model</td>
                    <td>@item.num_off</td>
                    <td>@item.parent_off</td>
                    <td>
                        <a asp-action="Details" asp-route-id="@item.id_zv" class="btn btn-primary">Подробнее</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>
